<?xml version="1.0" encoding="UTF-8" ?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:border="clr-namespace:Syncfusion.XForms.Border;assembly=Syncfusion.Core.XForms"
    x:Class="AceConstructionCalculator.Views.Creation.Steps.ProjectPriceConfig"
    xmlns:styles="clr-namespace:AceConstructionCalculator.Styles">

    <ContentView.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <styles:EntryStyles/>
                <styles:BorderStyles/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </ContentView.Resources>

    <ContentView.Content>

        <ScrollView>
            <StackLayout x:Name="contentStack"/>
        </ScrollView>
        <!--<border:SfBorder Style="{StaticResource DataFormBorderlessEntryBorderStyle}">



        </border:SfBorder>-->
    </ContentView.Content>
</ContentView>
